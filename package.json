{
  "name": "hjobs-admin",
  "version": "1.0.0",
  "description": "HJobs Admin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Granze/react-starterify.git"
  },
  "dependencies": {
    "aws-sdk": "^2.56.0",
    "bootstrap": "3",
    "clone": "^2.1.1",
    "flatpickr": "^2.5.8",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.10",
    "react-dom": "^15.5.4",
    "react-flatpickr": "^3.3.0",
    "react-loading": "0.0.9",
    "react-router-dom": "^4.1.1",
    "reflux": "^6.4.1",
    "semantic-ui-react": "^0.68.2"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "http://admin.hjobs.hk",
  "author": "Edmund",
  "license": "MIT",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
